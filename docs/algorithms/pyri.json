{
  "guid": "pyri",
  "name": "Poly Resonator",
  "categories": [
    "Effect",
    "Resonator",
    "Polysynth"
  ],
  "description": "A polyphonic synthesizer where each voice is an implementation of the open-source Rings module by Emilie Gillet. Supports microtuning.",
  "specifications": [
    {
      "name": "Voices",
      "value": {
        "minValue": 1,
        "maxValue": 8,
        "defaultValue": 1
      },
      "description": "The number of simultaneous voices (each a Rings instance)."
    }
  ],
  "parameters": [
