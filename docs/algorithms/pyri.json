      "name": "Mode",
      "unit": null,
      "defaultValue": 0,
      "minValue": 0,
      "maxValue": 4,
      "description": "Selects the resonator mode (like Rings).",
      "scope": "global"
    },
    {
      "name": "Synth effect",
      "unit": null,
      "defaultValue": 0,
      "minValue": 0,
      "maxValue": 1,
      "description": "Selects the audio effect applied if Mode is 'Synth'.",
      "scope": "global"
    },
